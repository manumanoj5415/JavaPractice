package br;

import java.util.*;
public class addingStringNumber {
	
	public static void main(String[] args) {
		boolean line;
		int lineNumber = 1;

		
		
		System.out.println("enter a number");
		Scanner scan = new Scanner(System.in);
		
		while(scan.hasNextLine()) {
			line = scan.hasNext();
			
			System.out.println(lineNumber + " " + line);
			lineNumber++;
		}
	} 
}
